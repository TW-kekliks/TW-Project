@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en">

<head>
    @Html.Partial("_Header")
</head>

<body>

    @Html.Partial("_Spinner")

    @Html.Partial("_Topbar")

    @Html.Partial("_Navbar")

    @Html.Partial("_Search")

    @Html.Partial("_Carousel")
    
    @Html.Partial("_Banner")

    @Html.Partial("_Footer")


    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-primary btn-lg-square rounded back-to-top"><i class="bi bi-arrow-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    @Scripts.Render("~/bundles/wow/js")
    @Scripts.Render("~/bundles/easing/js")
    @Scripts.Render("~/bundles/waypoints/js")
    @Scripts.Render("~/bundles/owlcarousel/js")
    @Scripts.Render("~/bundles/moment/js")
    @Scripts.Render("~/bundles/-timezone/js")
    @Scripts.Render("~/bundles/tempusdominus/js")
    @Scripts.Render("~/bundles/jqueryevent/js")
    @Scripts.Render("~/bundles/twentytwenty/js")

    <!-- Template Javascript -->
    @Scripts.Render("~/js/main.js")
</body>

</html>